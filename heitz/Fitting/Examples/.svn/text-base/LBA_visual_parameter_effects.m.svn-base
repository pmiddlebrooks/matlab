a = start.a;
for b = start.b:ub.b
    A = start.A;  
    %b = start.b;
    v = start.v;
    s = start.s;
    T0 = start.T0;
    
    %a = start.a;
    Ter = start.Ter;
    eta = start.eta;
    z = start.z;
    sZ = start.sZ;
    st = start.st;
    nu = start.nu;
    
    a = a + .001;
    
    srt = srt - T0;
    
    
    winner_LBA = cumsum(linearballisticPDF(srt,A,b,v,s) .* (1-linearballisticCDF(srt,A,b,1-v,s)));
    loser_LBA = cumsum(linearballisticPDF(srt,A,b,1-v,s) .* (1-linearballisticCDF(srt,A,b,v,s)));
    
    winner_DDM = CDFDif(SRT(:,1)./1000,1,[a Ter eta z sZ st nu]);
    loser_DDM = CDFDif(SRT(:,1)./1000,0,[a Ter eta z sZ st nu]);
    
    t = (1:1000) + T0;
    T = (.001:.001:1);
    
    subplot(121)
    plot(t,winner_LBA,'k',t,loser_LBA,'r')
    text(500,.5,['A = ' mat2str(round(A*100)/100)])
    text(500,.45,['b = ' mat2str(round(b*100)/100)])
    text(500,.4,['v = ' mat2str(round(v*100)/100)])
    text(500,.35,['s = ' mat2str(round(s*100)/100)])
    text(500,.3,['T0 = ' mat2str(round(T0*100)/100)])
    xlim([0 1000])
    ylim([0 1])
    
    subplot(122)
    plot(T,winner_DDM,'k',T,loser_DDM,'r')
    text(.500,.7,['a = ' mat2str(round(a*100)/100)])
    text(.500,.65,['Ter = ' mat2str(round(Ter*100)/100)])
    text(.500,.6,['eta = ' mat2str(round(eta*100)/100)])
    text(.500,.55,['z = ' mat2str(round(z*100)/100)])
    text(.500,.5,['sZ = ' mat2str(round(sZ*100)/100)])
    text(.500,.45,['st = ' mat2str(round(st*100)/100)])
    text(.500,.4,['nu = ' mat2str(round(nu*100)/100)])
    xlim([0 1])
    ylim([0 1])
    
    
    pause(.001)
    cla
    
    subplot(121)
    cla
    
end

