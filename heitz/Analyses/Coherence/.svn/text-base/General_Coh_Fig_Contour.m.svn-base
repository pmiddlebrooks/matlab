ylim([0 100])

% Create contour
contour(xdata1,ydata1,zdata1,'LineColor',[0 0 0],'Fill','on','Parent',axes1);

% Create colorbar
colorbar('peer',axes1,'LineWidth',2,'FontWeight','bold','FontSize',14);

